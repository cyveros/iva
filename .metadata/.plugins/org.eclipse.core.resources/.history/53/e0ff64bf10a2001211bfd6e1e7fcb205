import java.awt.Point;


public class lineVector {
	private Point p;
	private int x, y, width, height;
	
	public lineVector(Point p, int x, int y, int width, int height){
		
		this.p = p;
		this.x = x;
		this.y = y;
		this.width = width;
		this.height = height;
		
	}
	
	public Point[] getBoundaryCoordinates(){
		
	}
}
